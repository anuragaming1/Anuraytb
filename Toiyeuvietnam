    while true do
        for _, v in ipairs(weapons) do
            pcall(function()
                for _, tool in pairs(lp.Backpack:GetChildren()) do
                    if tool:IsA("Tool") and string.find(tool.Name:lower(), v:lower()) then
                        lp.Character.Humanoid:EquipTool(tool)
                        task.wait(0.5)
                        break
                    end
                end
            end)
        end
        task.wait(5)
    end
end)

